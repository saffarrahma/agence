{% extends "FaculteAdminBundle::layout.html.twig" %}

{% block content %}


<div class="page-content">
    <!-- BEGIN BREADCRUMBS -->
    <div class="breadcrumbs">
        <h1>Tableau de bord</h1>
    </div>
    <!-- END BREADCRUMBS -->
    <!-- BEGIN PAGE BASE CONTENT -->
    <!-- BEGIN DASHBOARD STATS 1-->

    {% set NbrProfessionel = 0 %}
    {% for Professionels in Professionel %}
        {% set NbrProfessionel = NbrProfessionel+1 %}
    {% endfor %}
    {% set NbrAssociation = 0 %}
    {% for Associations in Association %}
        {% set NbrAssociation = NbrAssociation+1 %}
    {% endfor %}
    {% set NbrParticulier = 0 %}
    {% for Particuliers in Particulier %}
        {% set NbrParticulier = NbrParticulier+1 %}
    {% endfor %}



    <div class="row" style="margin-left: 10%">
        <div class="col-md-3">
            <div class="circle-tile">
                <a href="#">
                    <div class="circle-tile-heading blue">
                        <i class="fa fa-users fa-fw fa-3x" style="margin-top: 35%"></i>
                    </div>
                </a>
                <div class="circle-tile-content blue">
                    <div class="circle-tile-description text-faded">
                        Professionel
                    </div>
                    <div class="circle-tile-number text-faded">
                        <span data-counter="counterup" data-value="{{ NbrProfessionel }}"></span>
                        <span id="sparklineA"></span>
                    </div>
                    <a href="{{ path('faculte_admin_professionels') }}" class="circle-tile-footer">Plus de détails <i class="fa fa-chevron-circle-right"></i></a>
                </div>
            </div>
        </div>
        <div class="col-md-3">
            <div class="circle-tile">
                <a href="#">
                    <div class="circle-tile-heading orange">
                        <i class="fa fa-users fa-fw fa-3x" style="margin-top: 35%"></i>
                    </div>
                </a>
                <div class="circle-tile-content orange">
                    <div class="circle-tile-description text-faded">
                       Particulier
                    </div>
                    <div class="circle-tile-number text-faded">
                        <span data-counter="counterup" data-value="{{ NbrParticulier }}"></span>
                        <span id="sparklineA"></span>
                    </div>
                    <a href="{{ path('faculte_admin_particuliers') }}" class="circle-tile-footer">Plus de détails <i class="fa fa-chevron-circle-right"></i></a>
                </div>
            </div>
        </div>
        <div class="col-md-3">
            <div class="circle-tile">
                <a href="#">
                    <div class="circle-tile-heading red">
                        <i class="fa fa-users fa-fw fa-3x" style="margin-top: 35%"></i>
                    </div>
                </a>
                <div class="circle-tile-content red">
                    <div class="circle-tile-description text-faded">
                        Association
                    </div>
                    <div class="circle-tile-number text-faded">
                            <span data-counter="counterup" data-value="{{ NbrAssociation }}"></span>
                        <span id="sparklineD"></span>
                    </div>
                    <a href="{{ path('faculte_admin_associations') }}" class="circle-tile-footer">Plus de détails <i class="fa fa-chevron-circle-right"></i></a>
                </div>
            </div>
        </div>
    </div>

<!-- END DASHBOARD STATS 1-->



    {% endblock %}

